{
  "installCommand": "corepack enable pnpm && pnpm install --no-frozen-lockfile",
  "buildCommand": "pnpm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "env": {
    "NODE_VERSION": "18",
    "NPM_CONFIG_REGISTRY": "https://registry.npmjs.org/",
    "NODE_OPTIONS": "--max-old-space-size=4096"
  }
}
